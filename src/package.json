{
  "name": "typescript-hello-world",
  "version": "1.0.0",
  "description": "A simple TypeScript Hello World project",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "example": "tsc && node dist/example.js",
    "policy-example": "tsc && node dist/policy-example.js",
    "server": "tsc && node dist/server.js",
    "server:dev": "tsc && NODE_ENV=development node dist/server.js",
    "clean": "rmdir /s /q dist"
  },
  "dependencies": {
    "@types/mongoose": "^5.11.96",
    "aws-sdk": "^2.1691.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "mongoose": "^8.18.1",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
